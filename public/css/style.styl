.container {
  padding-left: 0px;
  padding-right: 0px;
  margin: 0px;
  margin-left: 0px;
  margin-right: 0px;
}


.junctionlinkcenter{
  text-align: center;
  background-image: url("/images/bgimg-1.2.1.jpg");
  background-repeat: repeat-x;
  margin-left: -100px;
  margin-bottom: 20px;
}

.separator1{
  background-image: url("/images/bar.jpg");
  background-repeat: repeat;
  padding-bottom: 20px;
  margin-bottom: 25px;
  width: 100%;
  margin-left: auto;
  margin-right: auto;
}

.junctionlink { 
  width: 300px;
  height: 224px; 
  margin-left: auto;
  margin-right: auto;
  display: block;
  cursor: pointer;
  opacity: 1.0;
}
.junctionlink img.normal,
.junctionlink:hover img.hover { display: block; }
.junctionlink  img.hover,
.junctionlink:hover img.normal { display: none; }


.adOnIndexPage{
  max-width: 70%;
  height: auto;
  padding-top: 40px;
  padding-bottom: 40px;
  margin-left: auto;
  margin-right: auto;
  display: block;


}

.grandopening {
  margin-left: auto;
  margin-right: auto;
  width: 300px;
  height: auto;
  display: block;

}


.iframeclass {
  width: 100%;
  height: 480px;
  scrolling: no;
  frameborder: 2;
  margin-top: 10px;
  margin-bottom: 10px;
}

.floatingSignupBar {
    visibility: hidden;
    position: fixed; 
    bottom: 0;
    margin-left: auto;
    margin-right: auto;
    width: 400px;
    height: 150px;
    float: left;
}

.whiteBG {
    background: none no-repeat scroll 0 0 #fff;
}

.at4-follow-container addthis_24x24_style {
  margin-top: 5px;
}
